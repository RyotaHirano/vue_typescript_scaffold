{
  "name": "vue_typescript_scaffold",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --progress --open --config webpack.config/webpack.development.config.js",
    "clean": "rm -rf ./build",
    "lint": "prettier-eslint --write \"src/**/*.ts\"",
    "build": "yarn run clean && yarn run lint && webpack --progress --hide-modules --config webpack.config/webpack.production.config.js"
  },
  "engines": {
    "node": "^8.4.0"
  },
